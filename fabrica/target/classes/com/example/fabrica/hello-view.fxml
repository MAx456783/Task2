<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox alignment="CENTER" prefHeight="553.0" prefWidth="487.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fabrica.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="514.0" prefWidth="413.0">
<children>
    <Label fx:id="welcomeText" />
    <Canvas fx:id="canvas" height="335.0" layoutX="32.0" layoutY="45.0" onMouseClicked="#onDrawClick" width="371.0" />
    <ColorPicker fx:id="colorpicker" layoutX="32.0" layoutY="411.0">
      <value>
         <Color red="1.0" />
      </value></ColorPicker>
    <TextField fx:id="fabrica" layoutX="236.0" layoutY="455.0" onKeyReleased="#drawShape" promptText="number" />
         <Label alignment="TOP_CENTER" layoutX="18.0" layoutY="467.0" prefHeight="22.0" prefWidth="173.0" text="Введите номер фигуры" />
         <TextField fx:id="textfield" layoutX="237.0" layoutY="411.0" promptText="weight" />
</children>
    </AnchorPane>
</VBox>
